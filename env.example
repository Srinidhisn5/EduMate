# AI API Configuration

# OpenAI API (for premium AI responses)
# Get your API key from: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Hugging Face API (FREE - recommended)
# Get your free API token from: https://huggingface.co/settings/tokens
VITE_HF_API_TOKEN=your_huggingface_api_token_here

# Optional: Custom API endpoints
# VITE_OPENAI_API_BASE_URL=https://api.openai.com/v1
# VITE_HF_API_BASE_URL=https://api-inference.huggingface.co/models

# App configuration
VITE_APP_NAME=EduMate
VITE_APP_VERSION=1.0.0

# Feature flags
VITE_ENABLE_WIKIPEDIA=true
VITE_ENABLE_HUGGINGFACE=true
VITE_ENABLE_OPENAI=true 